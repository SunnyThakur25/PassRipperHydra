#Environment variables for PassRipperHydra
#Captcha solving service API keys

2CAPTCHA_API_KEY=your_2captcha_api_key_here
ANTI_CAPTCHA_API_KEY=your_anti_captcha_api_key_here
#Proxy provider API keys

BRIGHTDATA_API_KEY=your_brightdata_api_key_here
PROXYRACK_API_KEY=your_proxyrack_api_key_here
#Path to pretrained captcha CNN model

CAPTCHA_MODEL_PATH=models/captcha_cnn.h5
#Logging and output directories (optional, can be hardcoded in code)

LOG_DIR=output/logs
OUTPUT_DIR=output/results
#Streamlit configurations (optional)

STREAMLIT_SERVER_PORT=8501
STREAMLIT_SERVER_HEADLESS=true